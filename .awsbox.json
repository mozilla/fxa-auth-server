{
  "ami": "ami-91a2c8f8",
  "processes": [
    "bin/key_server.js"
  ],
  "env": {
    "CONFIG_FILES": "$HOME/code/config/awsbox.json,$HOME/config.json",
    "ES_HEAP_SIZE": "32m",
    "HEKAD_CONFIG": "$HOME/code/heka/hekad.toml"
  },
  "hooks": {
    "postcreate": "scripts/awsbox/post_create.sh",
    "postdeploy": "scripts/awsbox/post_deploy.sh"
  },
  "packages": [
    "mysql-server",
    "memcached",
    "nginx",
    "postfix"
  ]
}
