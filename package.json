    "jws": "3.1.3",
    "leftpad": "0.0.0",
    "load-grunt-tasks": "3.5.2",
    "mailparser": "0.6.1",
    "mkdirp": "0.5.1",
    "mocha": "3.1.2",
    "nock": "8.0.0",
    "npmshrink": "1.0.1",
    "nyc": "8.4.0",
    "proxyquire": "1.8.0",
    "simplesmtp": "0.3.35",
    "sinon": "1.17.5",
    "sjcl": "1.0.6",
    "ws": "1.1.1"
  }
}
